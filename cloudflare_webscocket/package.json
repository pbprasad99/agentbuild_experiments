{
  "name": "cloudflare-websocket-sec",
  "version": "1.0.0",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "typescript": "^5.0.0",
    "wrangler": "^4.54.0"
  },
  "dependencies": {
    "ws": "^8.14.2",
    "node-fetch": "^3.3.2"
  },
  "private": true,
  "scripts": {
    "start": "wrangler dev",
